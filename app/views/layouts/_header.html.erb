<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to t("static_pages.application.link_to_1"), root_path, class: "logo" %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to t("static_pages.application.link_to_2"), root_path %></li>
        <li><%= link_to t("static_pages.application.link_to_3"),  help_path %></li>
        <% if logged_in? %>
          <li><%= link_to t("static_pages.header.users"), users_path %></li>
          <li class="dropdown">
            <%= link_to "#", class: "dropdown-toggle", data: {toggle: "dropdown"} do %>
              <%= t "static_pages.header.acc" %> <b class="caret"></b>
            <% end %>
            <ul class="dropdown-menu">
              <li><%= link_to t("static_pages.header.prf"), current_user %></li>
              <li><%= link_to t("static_pages.header.sett"), edit_user_path(current_user) %></li>
              <li class="divider"></li>
              <li>
                <%= link_to t("static_pages.header.logout"), logout_path, method: :delete %>
              </li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to t("static_pages.header.log_in"), login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
